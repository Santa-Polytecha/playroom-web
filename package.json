{
	"name": "playroom-web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"postcss": "postcss --no-map --use autoprefixer --config bootstrap/build/js/postcss.js --replace bootstrap/dist/*/bootstrap.css"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"core-js": "^3.3.2",
		"v-emoji-picker": "^1.2.6",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3",
		"vue-socket.io": "^3.0.7",
		"vuex": "3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.0.0",
		"@vue/cli-plugin-eslint": "^4.0.0",
		"@vue/cli-service": "^4.0.0",
		"autoprefixer": "^7.1.2",
		"babel-eslint": "^10.0.3",
		"bootstrap": "^4.3.1",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"font-awesome": "~4.7.0",
		"git-label-packages": "^1.1.0",
		"grunt": "^1.0.4",
		"grunt-contrib-clean": "^1.0.0",
		"grunt-contrib-concat": "^1.0.1",
		"grunt-contrib-connect": "^1.0.2",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-uglify": "^2.0.0",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-exec": "^1.0.1",
		"grunt-sass": "^3.0.2",
		"jquery": "^3.4.1",
		"node-sass": "^4.12.0",
		"popper.js": "^1.14.3",
		"postcss-cli": "^6.1.2",
		"prettier": "1.18.2",
		"sass": "^1.23.0",
		"sass-loader": "^8.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off",
			"no-unused-vars": "off",
			"no-empty": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"prettier": {
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": false,
		"printWidth": 120,
		"jsxBracketSameLine": true
	}
}
