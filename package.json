{
	"name": "playroom-web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"core-js": "^3.3.2",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3",
		"vuex": "3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.0.0",
		"@vue/cli-plugin-eslint": "^4.0.0",
		"@vue/cli-service": "^4.0.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"vue-template-compiler": "^2.6.10",
		"prettier": "1.18.2",
		"sass": "^1.23.0",
		"sass-loader": "^8.0.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off",
			"no-unused-vars": "off",
			"no-empty": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"prettier": {
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": false,
		"printWidth": 120,
		"jsxBracketSameLine": true
	}
}
